<!DOCTYPE html>
<html>
<head>
<link rel="stylesheet" type="text/css" href="{site_url}/evlist/css/style.css" />
<meta name="robots" content="noindex">
</head>
<body>
<h2>{site_name}</h2>
<center>
  <h2 class="evlist_header">{today_str}</h2>
</center>

<table cellpadding="5" cellspacing="1" border="1" class="evlist_month">
  <tr class="daynames">
    <td>{dayname0}</td>
    <td>{dayname1}</td>
    <td>{dayname2}</td>
    <td>{dayname3}</td>
    <td>{dayname4}</td>
    <td>{dayname5}</td>
    <td>{dayname6}</td>
  </tr>
  <!-- BEGIN weekBlock -->
  <tr>
    <!-- BEGIN dayBlock -->
    <td valign="top" class="month{cal_day_style}" >
      <div class="monthview_daterow">
        <span class="date_number">
          {cal_day_anchortags}
        </span>
      </div>
      <div class="monthview_events">
        {cal_day_entries}
      </div>
    </td>
    <!-- END dayBlock -->
  </tr>
  <!-- END weekBlock -->
</table>
<i>{site_slogan}</i>
</body>
</html>
